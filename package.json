{
  "name": "pokedex",
  "description": "A Pokedex made with VueJs.",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "yarn build:prod",
    "build:prod": "rimraf dist && cross-env NODE_ENV=production vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "engines": {
    "node": ">=v14",
    "npm": ">=1.22"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rodgeraraujo/pokedex.git"
  },
  "keywords": [
    "vue",
    "pokedex"
  ],
  "author": "Rogério Araújo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rodgeraraujo/pokedex/issues"
  },
  "homepage": "https://github.com/rodgeraraujo/pokedex#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "colorthief": "^2.3.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "lodash": "^4.17.19",
    "path": "^0.12.7",
    "vue": "^2.6.11",
    "vue-axios": "^2.1.5",
    "vue-router": "^3.3.2",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "rimraf": "^3.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
